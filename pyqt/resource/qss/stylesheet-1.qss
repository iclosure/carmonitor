/** CurveWidget **/

CurveWidget {
    border-radius: 3px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(25, 25, 25, 220), stop: 1 rgba(25, 25, 25, 120));
}

QwtPlotCanvas {
    border-radius: 3px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(32, 32, 32, 200), stop: 1 rgba(32, 32, 32, 100));
}

QwtTextLabel {
    font: 16px "微软雅黑"
}

QwtScaleWidget {
    font: 12px "微软雅黑"
}

QwtLegendLabel{
    font: 10px "微软雅黑"
}

/** QLabel **/

QLabel {
    font: 17px "微软雅黑"
}

QwtPlotZoomer {
    color: white;
}

/** QGroupBox **/

QGroupBox {
    border: 1px solid gray;
    border-radius: 2px;
    margin-top: 3ex;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 0 3px;;
}

QGroupBox#groupBoxTop, QGroupBox#groupBoxStatus, QGroupBox#groupBoxCtrl {
    border: 0;
    background: transparent;
}

/** QPushButton **/

JDashButton {
    width: 100px;
    height: 50px;
    border-radius: 5px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop:0 rgba(64, 64, 64, 200), stop:0.7 rgba(12, 130, 166, 100), stop:1 rgba(12, 130, 166, 30));
    font: 17px "微软雅黑"
}

JDashButton:hover {
    border: 4px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop:0 rgba(64, 64, 64, 220), stop:0.7 rgba(12, 130, 166, 200), stop:1 rgba(12, 130, 166, 100));
}

JDashButton:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop:0 rgba(12, 120, 166, 254), stop:0.7 rgba(32, 150, 186, 100), stop:1 rgba(32, 150, 186, 30));
}

JDashButton:disable {
    width: 80px;
    border: 4px;
}

/** QSlider **/

QSlider {
    height: 55px;
}

QSlider::groove:horizontal {
    border-image: url(:/carmonitor/image/slider_bar-1.png) 1;
    border-width: 2px;
    border-radius: 2px;
    height: 8px;
 }

QSlider::handle:horizontal {
    image: url(:/carmonitor/image/slider_handle-1.png);
    width: 55px;
    margin: -32px -5px;
}
/*
QSlider::add-page:horizontal {
    background: lightsteelblue;
    border-radius: 6px;
}

QSlider::sub-page:horizontal {
    background: steelblue;
    border-radius: 6px;
}
*/
QSlider::groove:vertical {
    background: red;
    position: absolute;
    left: 4px; right: 4px;
}

QSlider::handle:vertical {
    height: 10px;
    background: green;
    margin: 0 -4px;
}

QSlider::add-page:vertical {
    background: white;
}

QSlider::sub-page:vertical {
    background: pink;
}

/** QLineEdit **/

QLineEdit  {
    background: rgba(64, 64, 64, 100);
    border-radius: 2px;
    border: 1px solid rgba(200, 200, 250, 30);
    color: rgba(200, 200, 200, 254);
    height: 25px;
    font: 16px "微软雅黑"
}

QLineEdit:read-only  {
    background: rgba(64, 64, 64, 100);
    border-radius: 2px;
    border: 0;
    color: rgba(200, 200, 200, 254);
}

QLineEdit:!read-only:hover {
    border: 1px solid rgba(200, 200, 250, 100);
}

QLineEdit:!read-only:focus {
    background: rgba(100, 100, 100, 20);
    border: 1px solid rgba(200, 200, 250, 150);
}


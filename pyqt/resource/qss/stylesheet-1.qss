/* QPushButton#buttonOk */

QPushButton#buttonOk {
    width: 80px;
    height: 30px;
    border-radius: 5px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(25, 25, 25, 220), stop: 1 rgba(25, 25, 25, 200));
}

QPushButton#buttonOk:hover {
    border: 4px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(25, 25, 25, 220), stop: 1 rgba(25, 25, 25, 240));
}


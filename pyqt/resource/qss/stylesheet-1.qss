    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
        stop: 0 rgba(25, 25, 25, 220), stop: 1 rgba(25, 25, 25, 250));
}

/* HistoryWidget */
HistoryWidget {
    background: #202020;
}

/* HistoryWidget::buttonQuit */

HistoryWidget QPushButton#buttonQuit {
    border-image: url(:/carmonitor/image/button-quit.png);
    width: 60px;
    height: 60px;
}

HistoryWidget QPushButton#buttonQuit:pressed,
HistoryWidget QPushButton#buttonQuit:hover {
    width: 60px;
    height: 60px;
}

/* QProgreeBar */

QProgressBar {
    border: 1px solid #c0c0c0;
    background-color: #e0e0e0;
    border-radius: 3px;
    text-align: center;
    height: 20px;
    color: darkblue;
}

QProgressBar::chunk {
    border: 1px solid #a0a0a0;
    background-color: rgba(80, 80, 80, 100);
    width: 5px;
    magin: 0.5px;
}

/* QScrollBar */

QScrollBar::vertical {
    background: rgba(204, 210, 220, 240);
    width: 8px;
    magin: 0;
    border-radius: 4px;
}

QScrollBar::handle:vertical {
    background: rgba(80, 80, 80, 100);
    min-height: 8px;
    magin: 0;
    border-radius: 4px;
}

QScrollBar::add-line:vertical {
    height: 0;
}

QScrollBar::sub-line:vertical {
    height: 0;
}

QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
    border: 0 solid gray;
    width: 0;
    height: 0;
    background: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}


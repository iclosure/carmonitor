
/** QWidget **/

QWidget {
    color: #5fd43b;
}

/** QMessageBox **/

QMessageBox QLabel,
QMessageBox QPushButton {
	color: darkblue;
}

/* QWidget#groupBoxMiddle */
QWidget#areaMiddle {
    border-radius: 2px;
    background: rgba(32, 32, 32, 100);
}

/* MainWidget */

MainWidget {
    background: #151515;
}


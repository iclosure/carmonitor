/** QTabWidget **/

QTabWidget::pane {
     border: 0px solid #32435E;
     position: absolute;
     left: -0.1em;
}

QTabWidget#MainTabWidget::tab-bar {
     left: -3px;
}
QTabWidget#MainTabWidget QTabBar::tab {
     height: 14ex;
     width: 14ex;
}

/** QTabWidget **/

QTabBar::tab {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #292F31, stop: 1 #0C131E);
}

QTabBar::tab:selected{
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #113845,  stop: 1.0 #15A8FF);
}

QTabBar::tab:hover {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #113845,  stop: 1.0 #0E6F80);
}

#QTabBar::tab:selected {
     border-color: #32435E;
     border-right-color: #32435E;
}

#QTabBar::tab:!selected {
     margin-left: 2px;
}

#QTabBar:tab:first:selected {
    margin-top: 0;
}

QTabBar:tab:last:selected {
    margin-right: 0;
}

QTabBar:tab:only-one {
     margin: 0;
}

/**  **/

QComboBox,
QSpinBox,
QDoubleSpinBox,
QDateTimeEdit {
    background: rgba(39, 39, 39, 50);
    border-radius: 2px;
    border: 1px solid rgba(200, 200, 250, 30);
    color: rgba(200, 200, 200, 254);
}

QComboBox:hover,
QSpinBox:hover,
QDateTimeEdit:hover,
QDoubleSpinBox#MyDoubleSpinBox:hover {
    border: 1px solid rgba(200, 200, 250, 100);
}

QComboBox::down-arrow {
    image:url(:/carmonitor/image/arrow_down.png);
}

QDateTimeEdit::drop-down,
QComboBox::drop-down,
QSpinBox::drop-down,
QDoubleSpinBox#MyDoubleSpinBox::drop-down {
    width: 14px;
    border-left: 1px solid rgba(128, 128, 128, 70);
    background:transparent;
}

QDateTimeEdit::drop-down:hover,
QComboBox::drop-down:hover,
QSpinBox::drop-down:hover,
QDoubleSpinBox#MyDoubleSpinBox::drop-down:hover {
    border-left: 1px solid lightgray;
    background:rgb(220, 240, 250);
}

/* ScrollBar */

QScrollBar:vertical {
    background: lightgrey;
    width: 12px;
}

QScrollBar::handle:vertical {
    background: rgba(80,80,80,70);
    min-height: 12px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::add-line:vertical {
    height: 0px;
}

QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    border: 0px solid grey;
    width: 2px;
    height: 2px;
    background: white;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal {
    background: lightgrey;
    height: 12px;
}

QScrollBar::handle:horizontal {
background: rgba(80,80,80,70);
    min-width: 12px;
    margin: 0px 0px 0px 0px;
}

QScrollBar::add-line:horizontal {
    width: 0px;
}

QScrollBar::sub-line:horizontal {
    width: 0px;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
    border: 0px solid grey;
    width: 2px;
    height: 2px;
    background: white;
}

QScrollBar::add-page:horzontal, QScrollBar::sub-page:horizontal {
    background: none;
}

